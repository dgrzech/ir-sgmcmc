{
	"name": "SG_MCMC",
	"data_loss": {
		"type": "GMM",
		"args": {
			"no_components": 4,
			"s": 3
		}
	},
	"data_loss_scale_prior": {
		"type": "LogScaleNormalPrior",
		"args": {
			"loc": 0.00,
			"scale": 2.30
		}
	},
	"data_loss_proportion_prior": {
		"type": "DirichletPrior",
		"args": {
			"num_classes": 4,
			"alpha": 0.5
		}
	},
	"reg_loss": {
		"type": "RegLoss_LogNormal",
		"args": {
			"diff_op": "GradientOperator",
			"learnable": true,
			"w_reg": 0.75
		}
	},
	"reg_loss_loc_prior": {
		"type": "LogEnergyExpGammaPrior",
		"args": {
			"w_reg": 0.75,
			"learnable": false
		}
	},
	"reg_loss_scale_prior": {
		"type": "LogScaleNormalPrior",
		"args": {
			"loc": 2.80,
			"scale": 5.00
		}
	},
	"entropy_loss": {
		"type": "EntropyMultivariateNormal",
		"args": {}
	},
	"transformation_module": {
		"type": "SVF_3D",
		"args": {}
	},
	"registration_module": {
		"type": "RegistrationModule",
		"args": {}
	},
	"optimizer_q_v": {
		"type": "Adam",
		"args": {
			"lr": 0.005
		}
	},
	"optimizer_SG_MCMC": {
		"type": "SGD",
		"args": {
			"lr": 0.25
		}
	},
	"data_loader": {
		"type": "BiobankDataLoader",
		"args": {
			"data_dir": "/vol/bitbucket/dig15/datasets/mine/biobank/biobank_01",
			"dims": [96, 96, 96],
			"sigma_v_init": 0.5,
			"u_v_init": 0.0
		}
	},
	"Sobolev_grad": {
		"enabled": true,
		"s": 7,
		"lambda": 0.5
	},
	"virtual_decimation": true,
	"trainer": {
		"VI": true,
		"MCMC": true,
		"uniform_noise": {
			"enabled": true,
			"magnitude": 0.1
		},
		"no_iters_VI": 1536,
		"no_samples_VI_test": 200,
		"no_iters_burn_in": 50000,
		"no_samples_MCMC": 200000,
		"log_period_VI": 128,
		"log_period_MCMC": 400,
		"tensorboard": true,
		"verbosity": 2,
		"save_dir": "saved/"
	}
}
